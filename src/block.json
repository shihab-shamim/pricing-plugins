{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "b-blocks/enque-test",
	"version": "1.0.0",
	"title": "price card",
	"category": "widgets",
	"description": "Short description of the Test Purpose",
	"keywords": [
		"price card"
	],
	"textdomain": "b-blocks",
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "center"
		},
		"cards": {
    "type": "array",
    "default": [
      {
        "title": "Starter",
        "price": "$19",
        "type": "/month",
        "buttonText": "Select Plan",
        "features": [
          "5GB Linux Web Space",
          "5 MySQL Databases",
          "Unlimited Email",
          "250Gb mo Transfer",
          "24/7 Tech Support",
          "Daily Backups"
        ]
      },
      {
        "title": "Basic",
        "price": "$29",
        "type": "/month",
        "buttonText": "Select Plan",
        "features": [
          "10GB Linux Web Space",
          "10 MySQL Databases",
          "Unlimited Email",
          "500Gb mo Transfer",
          "24/7 Tech Support",
          "Daily Backups"
        ]
      },
      {
        "title": "Specialist",
        "price": "$49",
        "type": "/month",
        "buttonText": "Select Plan",
        "features": [
          "25GB Linux Web Space",
          "25 MySQL Databases",
          "Unlimited Email",
          "2000Gb mo Transfer",
          "24/7 Tech Support",
          "Daily Backups"
        ],
        "featured": true
      },
      {
        "title": "Ultra",
        "price": "$99",
        "type": "/month",
        "buttonText": "Select Plan",
        "features": [
          "100GB Linux Web Space",
          "Unlimited MySQL Databases",
          "Unlimited Email",
          "10000Gb mo Transfer",
          "24/7 Tech Support",
          "Daily Backups"
        ]
      }
    ]
  },
  "layout": {
	"type":"object",
	"default":{
		"desktop":2,
		"tablet":2,
     "mobile":1
    

	}
  },
  "gaps":{
	"type":"string",
	"default":"0px"
  },
  "CardsContainerStyle":{
	"type":"object",
	"default":{
		"width":"100%",
		"height":"100%"
	}
  },
  "titleColor":{
    "type":"object",
    "default":{
      "color": "#ffffff",
      "bgType": "solid",
      "bg": "rgba(0, 0, 0, 0.5)",
      "gradient": "linear-gradient(135deg, #4527a4, #8344c5)"
    }
  },
  "titleAlign":{
    "type":"string",
    "default":"left"
  },
  "titleTypo":{
    "type":"object",
    "default":{
      "fontFamily": "Arial",
      "fontWeight": 400,
      "fontSize": {
        "desktop": "3em",
        "tablet": "2em",
        "mobile": "1em"
      },
      "fontStyle": "normal",
      "textTransform": "uppercase",
      "textDecoration": "none",
      "lineHeight": "150%",
      "letterSpace": "4px"
    }
  },
  "priceColor":{
    "type":"object",
    "default":{
      "color": "#ffffff",
      "bgType": "solid",
      "bg": "#5F3047"
    }
  },
  
  "priceAlign":{
    "type":"string",
    "default":"right"
  },
  "priceTypo":{
    "type":"object",
    "default":{
      "fontFamily": "",
      "fontWeight": 400,
      "fontSize": {
        "desktop": "3em",
        "tablet": "2em",
        "mobile": "1em"
      },
      "fontStyle": "normal",
      "textTransform": "uppercase",
      "textDecoration": "none",
      "lineHeight": "150%",
      "letterSpace": "4px"
    }
  },
  "featuresColor":{
    "type":"object",
    "default":{
      "color": "#ffffff",
      "bgType": "solid",
      "bg": "#5f3047"
    }
  }
  ,
  "featureTypo":{
    "type":"object",
    "default":{
      "fontFamily": "",
      "fontWeight": "",
      "fontSize": {
        "desktop": "0.75em",
        "tablet": "2em",
        "mobile": "1em"
      },
      "fontStyle": "normal",
      "textTransform": "",
      "textDecoration": "none",
      "lineHeight": "",
      "letterSpace": ""
    }
  },
  "buttonColor":{
    "type":"object",
    "default":{
      "color": "#ffffff",
      "bgType": "solid",
      "bg": "#442232"
    }
  },
  "ButtonTypo":{
    "type":"object",
    "default":{
      "fontFamily": "",
      "fontWeight": "600",
      "fontSize": {
        "desktop": "13px",
        "tablet": "13px",
        "mobile": "13px"
      },
      "fontStyle": "normal",
      "textTransform": "uppercase",
      "textDecoration": "none",
      "lineHeight": "",
      "letterSpace": "4px"
    }
  },
  "buttonPadding":{
    "type":"object",
    "default":{
      "top":"12px",
      "bottom":"12px",
      "left":"20px",
      "right":"20px"
    }

  },
  "buttonRadius":{
    "type":"object",
    "default":{
      "top":"30px",
      "bottom":"30px",
      "left":"30px",
      "right":"30px"
    }

  },
  "iconColor":{
    "type":"string",
    "default":"rgba(0, 0, 0, 0.5)"
  },
  "iconSize":{
    "type":"string",
    "default":"16px"
  },
  "cardBorder":{
    "type":"object",
    "default":{
      "width":"3px",
      "style":"solid",
      "color":"#442232"
    }
  },
  "cardsFather":{
    "type":"object",
    "default":{
      "type": "solid",
    "color": "#212121",
    "gradient": "",
    "image": {
      "url": "",
      "alt": ""
    },
    "position": "center center",
    "attachment": "initial",
    "repeat": "no-repeat",
    "size": "cover",
    "overlayColor": ""
    }
  },
  "cardShadow":{
    "type":"array",
    "default":[
      {
        "hOffset": "0px",
        "vOffset": "0px",
        "blur": "0px",
        "spreed": "5px",
        "color": "rgba(0, 0, 0, 0.3)",
        "isInset": false
      }
    ]
  },
  "cardRadius":{
    "type":"object",
    "default":{
      "top":"0px",
      "bottom":"0px",
      "left":"0px",
      "right":"0px"
    }

  }
	
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false
	},
	"example": {
		"attributes": {}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./view.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}